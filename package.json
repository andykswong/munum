{
  "name": "munum",
  "version": "0.2.0",
  "description": "Micro 3D Math Library for JavaScript and WebAssembly",
  "type": "module",
  "sideEffects": false,
  "main": "index.js",
  "ascMain": "assembly/index.ts",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./index.d.ts"
    },
    "./assembly": {
      "import": "./assembly"
    }
  },
  "files": [
    "/assembly",
    "/dist",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "prepublishOnly": "npm run build && npm test && npm run docs",
    "clean": "rimraf coverage/ docs/ dist/ target/ **/__tests__/**/*.spec.map **/__tests__/**/*.spec.wat",
    "prebuild": "npm run lint",
    "build": "npm run tsc",
    "lint": "eslint assembly --ext .ts,.tsx",
    "tsc": "npm run babel && npm run types",
    "babel": "cross-env-shell babel assembly -d dist --extensions '.ts' --out-file-extension .js",
    "types": "tsc -p ./assembly --emitDeclarationOnly && copyfiles -f assembly/types/index.d.ts dist/types",
    "test": "npm run asp && npm run jest",
    "asp": "asp -c as-pect.config.cjs",
    "jest": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest",
    "jest:watch": "npm run jest -- --watch",
    "docs": "typedoc --tsconfig assembly/tsconfig.json --out docs assembly/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andykswong/munum.git"
  },
  "keywords": [
    "numeric",
    "matrix",
    "math",
    "typescript",
    "assemblyscript",
    "webassembly",
    "wasm"
  ],
  "author": "Andy K.S. Wong <andykswong@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andykswong/munum/issues"
  },
  "homepage": "https://github.com/andykswong/munum",
  "devDependencies": {
    "@as-pect/cli": "^8.1",
    "@babel/cli": "^7.20",
    "@babel/core": "^7.20",
    "@babel/preset-env": "^7.20",
    "@babel/preset-typescript": "^7.20",
    "@types/jest": "^29.5",
    "@types/node": "^20.0",
    "@typescript-eslint/eslint-plugin": "^7.0",
    "@typescript-eslint/parser": "^7.0",
    "assemblyscript": "^0.27",
    "babel-jest": "^29.5",
    "babel-plugin-add-import-extension": "^1.6",
    "copyfiles": "^2.4",
    "cross-env": "^7.0",
    "eslint": "^8.50",
    "jest": "^29.5",
    "rimraf": "^5.0",
    "typedoc": "^0.25",
    "typescript": "^5.4"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.spec.ts"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "assembly/**/*.ts",
      "!**/__tests__/**"
    ],
    "coverageDirectory": "coverage"
  }
}
